{
  "name": "infralytics-charts",
  "version": "0.1.0",
  "description": "Plotly.js charts in React for Infralytics",
  "author": "Brandon M. Mitchell",
  "license": "MIT",
  "homepage": "https://github.com/brammitch/infralytics-charts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brammitch/infralytics-charts.git"
  },
  "bugs": {
    "url": "https://github.com/brammitch/infralytics-charts/issues"
  },
  "workspaces": [
    "example",
    "lib"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "plotly",
    "react",
    "infralytics",
    "data",
    "visualization"
  ],
  "scripts": {
    "build": "npm run clean && npm run build -w lib",
    "clean": "rimraf dist",
    "example": "npm run dev -w example",
    "watch": "npm run watch -w lib"
  },
  "dependencies": {
    "color": "4.2.3",
    "lodash.merge": "4.6.2",
    "plotly.js": "^2.12.0",
    "react-plotly.js": "^2.5.1"
  },
  "peerDependencies": {
    "react": ">=16.8.3",
    "react-dom": ">=16.8.3",
    "recharts": ">=1.8.0"
  },
  "devDependencies": {
    "@types/node": "16.11.36",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  }
}
